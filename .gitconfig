[alias]
	b = branch
	bl = branch -l
	blr = branch -l -r
	co = checkout
	cp = cherry-pick
	dd = difftool -d
	dds = difftool -d --cached
	df = difftool -y
	dfs = difftool -y --cached
	f = fetch -p
	mt = mergetool
	rbc = rebase --continue
	st = status

[core]
	editor = 'C:\\Program Files\\Microsoft VS Code Insiders\\Code - Insiders.exe' -n --wait

[diff]
	tool = bc3

[difftool "bc3"]
	path = c:/Program Files/Beyond Compare 4/BComp.exe

[merge]
	tool = bc3

[mergetool "bc3"]
	path = C:/Program Files/Beyond Compare 4/BComp.exe

[user]
	name = Jon Ganz
	email = TODO
    
[pull]
	rebase = true
